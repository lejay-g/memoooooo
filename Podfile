# Uncomment the next line to define a global platform for your project

target 'memoooooo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '12.0'
  # pod 'RealmSwift', '~> 5.5.0'
  pod 'RealmSwift'

end

# watchアプリ
target 'watch' do
  use_frameworks!
  platform :watchos, '7.0'
  # pod 'RealmSwift', '~> 5.5.0'
  pod 'RealmSwift'
end

# watchアプリのextension
target 'watch Extension' do
  use_frameworks!
  platform :watchos, '7.0'
  # pod 'RealmSwift', '~> 5.5.0'
  pod 'RealmSwift'
end
